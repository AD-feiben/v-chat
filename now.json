{
  "version": 2,
  "name": "VChat",
  "builds": [
    { "src": "dist/**/*", "use": "@now/static" },
    { "src": "server/index.js", "use": "@now/node-server" }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "dist/.*"
    },
    {
      "src": "/socket.io",
      "dest": "server/index.js"
    }
  ]
}